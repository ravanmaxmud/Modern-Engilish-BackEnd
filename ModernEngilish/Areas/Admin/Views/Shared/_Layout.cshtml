    <script src="~/admin/assets/js/DropZone.js" asp-append-version="true"></script>
    <script src="~/admin/assets/js/app.js" asp-append-version="true"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/microsoft-signalr/6.0.1/signalr.js"></script>

    <script src="~/admin/assets/js/hubs/alert-handler.js" asp-append-version="true"></script>
    <!-- Custom Js -->
</body>
