                                    </li>
                                </ul><!--end nav-->
                            </div><!--end sidebarAnalytics-->
                        </li><!--end nav-item-->
                        <li class="nav-item">
                            <a class="nav-link" asp-route="admin-aged-list">
                                Ages
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#sidebarAbout" data-bs-toggle="collapse" role="button"
                               aria-expanded="false" aria-controls="sidebarAbout">
                                Gallery
                            </a>
                            <div class="collapse " id="sidebarAbout">
                                <ul class="nav flex-column">
                                    <li class="nav-item">
                                        <a asp-route="admin-partie-list" class="nav-link">Galleries</a>
                                    </li>
                                </ul><!--end nav-->
                            </div><!--end sidebarAnalytics-->
                        </li><!--end nav-item-->

                    </ul>

                    @*        <ul class="nav flex-column">
                    <li class="nav-item">
                    <a asp-route="admin-nav-list" class="nav-link">NAVBARS</a>
                    </li>
                    <li class="nav-item">
                    <a asp-route="admin-slider-list" class="nav-link">SLIDERS</a>
                    </li>
                    <li class="nav-item">
                    <a asp-route="admin-payment-list" class="nav-link">PAYMNET-BENEFITS</a>
